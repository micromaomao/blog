{
	"compilerOptions": {
		"strict": false,
		"sourceMap": true,
		"module": "ESNext",
		"target": "ESNext",
		"lib": [
			"ESNext",
			"DOM"
		],
		"outDir": "./dist/",
		"typeRoots": [
			"./node_modules/@types",
			"./node_modules/@svgdotjs"
		],
		"baseUrl": ".",
		"allowJs": true,
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"verbatimModuleSyntax": true,
		"jsx": "preserve",
		"paths": {
			"js/*": [
				"./js/*"
			],
		}
	}
}
