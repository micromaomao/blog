{
  "name": "blog-generator",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "watch": "env NODE_ENV=development BACKEND_ENDPOINT=http://localhost:3000 nodemon -e '*.*' -i .git -i node_modules -i server -i dist -i .parcel-cache -i parcel-bundle-reports index.js",
    "watch-draft": "npm run watch -- -- --draft-mode",
    "build": "env NODE_ENV=production node index.js",
    "build-draft": "env NODE_ENV=production node index.js --draft-mode",
    "dev:server": "cd server && env NODE_ENV=development nodemon -w . -e '*.js' -e '*.ts' -x 'ts-node index.ts'",
    "start:server": "cd server && env NODE_ENV=production ts-node index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micromaomao/blog.git"
  },
  "author": "maowtm <m@maowtm.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micromaomao/blog/issues"
  },
  "homepage": "https://github.com/micromaomao/blog#readme",
  "devDependencies": {
    "@fluentui/react-components": "^9.46.1",
    "@fluentui/react-icons": "^2.0.226",
    "@parcel/config-default": "^2.11.0",
    "@parcel/core": "^2.11.0",
    "@parcel/reporter-bundle-analyzer": "^2.11.0",
    "@svgdotjs/svg.js": "^3.2.0",
    "@types/express": "^4.17.21",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.18",
    "mathjax-node": "^2.1.1",
    "nodemon": "^3.0.3",
    "parcel": "^2.11.0",
    "pug": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "pg": "^8.11.3"
  }
}
